@model IEnumerable<Order>

@if (Model.Any())
{
    foreach (var order in Model)
    {
        <div class="my-3">
            <partial name="_OrderDetails" model="@order"/>
        </div>
    }
}
else
{
    <div class="alert alert-info">
        <p class="lead">
            There is no order yet.
        </p>
    </div>
}