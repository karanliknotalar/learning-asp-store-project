@model IEnumerable<Category>

<div class="my-3">
    <ul class="list-group">
        @foreach (var category in Model)
        {
            <li class="list-group-item">
                <a class="nav-link" asp-controller="Product" asp-action="Index" asp-route-categoryId="@category.CategoryId">@category.CategoryName</a>
            </li>
        }
    </ul>
</div>